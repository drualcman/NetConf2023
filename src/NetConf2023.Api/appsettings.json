{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseOptions": {
    "ConnectionString": null,
    "Options": {
      "LogOptions": {
        "LogResults": true,
        "LogFolder": "logs"
      },
      "EnableSqlInjectionControl": true,
      "EnableCharChrControl": true
    }
  }
}
